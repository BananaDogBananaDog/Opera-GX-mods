{
    "manifest_version": 3,
    "name": "Hypixel SkyBlock Mod",
    "description": "The Hypixel skyblock i made putting in heart and soul, hoping everyone likes my effort :3",
    "developer":
    {
        "name": "Banana Dog"
    },
    "icons":
    {
        "399": "icon.png"
    },

    "mod":
    {
        "license": "license.txt",
        "payload":
        {
            "background_music":
            [
                "music/Abstract Ringing (Wilderness).mp3",
                "music/Ambient Caves (Deep Caverns).mp3",
                "music/Journey in the Sky (Spider Den).mp3",
                "music/Light From Afar (Farming Island).mp3",
                "music/Sky of Trees (Forest Islands).mp3"
            ],
            "browser_sounds":
            {
                "FEATURE_SWITCH_OFF":
                [
                    "sounds/feature_switch_off.mp3"
                ],
                "FEATURE_SWITCH_ON":
                [
                    "sounds/feature_switch_on.mp3"
                ],
                "HOVER":
                [
                    "sounds/hover.mp3"
                ],
                "TAB_CLOSE":
                [
                    "sounds/close_tab.mp3"
                ]
            },
            "keyboard_sounds":
            {
                "TYPING_BACKSPACE":
                [
                    "keyboard/backspace.mp3"
                ],
                "TYPING_ENTER":
                [
                    "keyboard/enter.mp3"
                ],
                "TYPING_LETTER":
                [
                    "keyboard/allkeys.mp3"
                ]
            },
            "page_styles":
            [
                {
                    "css":
                    [
                        "webmodding/opera.css"
                    ],
                    "matches":
                    [
                        "https://*.opera.com/*"
                    ],

                    "css":
                    [
                        "webmodding/changecolors.css",
                        "webmodding/main.theme.css"
                    ],
                    "matches":
                    [
                        "https://*.discord.com/*"
                    ]
                }
                
            ],
            "shaders":
            {
                "Pixelate":
                {
                    "path": "shader/pixelate.txt"
                }                
            },
            "theme":
            {
                "dark":
                {
                    "gx_accent":
                    {
                        "h": 43,
                        "s": 100,
                        "l": 48
                    },
                    "gx_secondary_base":
                    {
                        "h": 25,
                        "s": 75.9,
                        "l": 31
                    }
                },
                "light":
                {
                    "gx_accent":
                    {
                        "h": 43,
                        "s": 100,
                        "l": 48
                    },
                    "gx_secondary_base":
                    {
                        "h": 25,
                        "s": 75.9,
                        "l": 31
                    }
                }
            },
            "wallpaper":
            {
                "dark":
                {
                    "image": "wallpaper/video.mp4",
                    "first_frame": "wallpaper/first_frame.png",
                    "text_color": "#FFFFFF",
                    "text_shadow": "#0B000E"
                },
                "light":
                {
                    "image": "wallpaper/video.mp4",
                    "first_frame": "wallpaper/first_frame.png",
                    "text_color": "#FFFFFF",
                    "text_shadow": "#0B000E"
                }
            }
        },
        "schema_version": 1
    },
    "version": "1.0"
}
